<%= form_for @blog, url:blog_path(@blog), method: :put do |f| %>
  <h3>Title</h3>
  <div class="form-group">
    <%= f.text_field :title %>
  </div>
  <h3>Content</h3>
  <div class="form-group">
    <%= f.text_area :content %>
  </div>
  <div class="form-group">
    <%= f.submit %>
  </div>
<% end %>
